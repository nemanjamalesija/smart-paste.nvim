{
  "runtime": {
    "version": "LuaJIT"
  },
  "workspace": {
    "library": [
      "$VIMRUNTIME"
    ],
    "checkThirdParty": "Disable"
  },
  "diagnostics": {
    "globals": [
      "describe",
      "it",
      "before_each",
      "after_each",
      "assert"
    ],
    "disable": [
      "luadoc-miss-see-name"
    ]
  }
}
